<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/x40"
    android:layout_marginLeft="@dimen/x14"
    android:layout_marginRight="@dimen/x14"
    android:orientation="horizontal">


    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/table_line" />

    <TextView
        android:id="@+id/tv_jiang"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/tickets_yi"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/text_size_14"
        />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/table_line" />

    <TextView
        android:id="@+id/tv_money"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/tickets_100"
        android:textColor="@color/color_919191"
        android:textSize="@dimen/text_size_14" />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/table_line" />

    <TextView
        android:id="@+id/tv_num"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/tickets_100"
        android:textColor="@color/color_919191"
        android:textSize="@dimen/text_size_14" />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/table_line" />

    <TextView
        android:id="@+id/tv_gailv"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center"
        android:text="@string/tickets_100"
        android:textColor="@color/color_919191"
        android:textSize="@dimen/text_size_14" />

    <View
        android:layout_width="1px"
        android:layout_height="match_parent"
        android:background="@color/table_line" />

</LinearLayout>
