package com.yst.onecity.bean.livevideo;

import java.util.List;

/**
 * 视频直播实体
 *
 * @author WangJingWei
 * @version 4.1.0
 * @date 2018/5/14.
 */
public class LiveVideoBean {

    private int code;
    private String msg;
    private List<ContentBean> content;

    public int getCode() {
        return code;
    }

    public void setCode(int code) {
        this.code = code;
    }

    public String getMsg() {
        return msg;
    }

    public void setMsg(String msg) {
        this.msg = msg;
    }

    public List<ContentBean> getContent() {
        return content;
    }

    public void setContent(List<ContentBean> content) {
        this.content = content;
    }

    public static class ContentBean {
        private String id;
        private String imgUrl;
        private String type;
        private String title;

        /**
         * 视频部分
         */
        private String views;

        /**
         * 直播部分
         */
        private String onLine;
        private String rtmpPlayAddress;
        private String flvPlayAddress;
        private String hlsPlayAddress;
        private String roomId;
        private String groupId;
        private String liveAddress;

        public String getId() {
            return id;
        }

        public void setId(String id) {
            this.id = id;
        }

        public String getImgUrl() {
            return imgUrl;
        }

        public void setImgUrl(String imgUrl) {
            this.imgUrl = imgUrl;
        }

        public String getDesc() {
            return title;
        }

        public void setDesc(String desc) {
            this.title = desc;
        }

        public String getOnLineNum() {
            return onLine;
        }

        public void setOnLineNum(String onLineNum) {
            this.onLine = onLineNum;
        }

        public String getPlayNum() {
            return views;
        }

        public void setPlayNum(String playNum) {
            this.views = playNum;
        }

        public String getType() {
            return type;
        }

        public void setType(String type) {
            this.type = type;
        }

        public String getRtmpPlayAddress() {
            return rtmpPlayAddress;
        }

        public void setRtmpPlayAddress(String rtmpPlayAddress) {
            this.rtmpPlayAddress = rtmpPlayAddress;
        }

        public String getFlvPlayAddress() {
            return flvPlayAddress;
        }

        public void setFlvPlayAddress(String flvPlayAddress) {
            this.flvPlayAddress = flvPlayAddress;
        }

        public String getHlsPlayAddress() {
            return hlsPlayAddress;
        }

        public void setHlsPlayAddress(String hlsPlayAddress) {
            this.hlsPlayAddress = hlsPlayAddress;
        }

        public String getRoomId() {
            return roomId;
        }

        public void setRoomId(String roomId) {
            this.roomId = roomId;
        }

        public String getGroupId() {
            return groupId;
        }

        public void setGroupId(String groupId) {
            this.groupId = groupId;
        }

        public String getLiveAddress() {
            return liveAddress;
        }

        public void setLiveAddress(String liveAddress) {
            this.liveAddress = liveAddress;
        }
    }
}
