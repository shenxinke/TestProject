<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="370dp"
    android:paddingBottom="@dimen/margin_15dp"
    >

    <com.yst.onecity.view.roundedimageview.RoundedImageView
        android:id="@+id/more_item_head"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:scaleType="fitXY"
        android:src="@mipmap/brand_default"
        app:border_color="@color/color_dddddd"
        app:corner_radius="@dimen/margin_10dp"
        android:layout_marginTop="@dimen/margin_10dp"
        android:layout_marginLeft="@dimen/margin_10dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/margin_10dp" />

    <TextView
        android:id="@+id/more_item_name"
        android:text="只缘身在此山中"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textSize="13sp"
        android:layout_marginTop="@dimen/margin_5dp"
        android:layout_marginLeft="@dimen/margin_10dp"
        android:maxEms="6"
        android:singleLine="true"
        android:ellipsize="end"
        app:layout_constraintTop_toTopOf="@+id/more_item_head"
        app:layout_constraintLeft_toRightOf="@+id/more_item_head"
        android:layout_marginStart="@dimen/margin_10dp" />

    <TextView
        android:id="@+id/more_item_text"
        android:text="以人才为跟本,以市场为向导，以质量为保证！"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="11sp"
        android:maxLines="1"
        android:maxEms="18"
        android:ellipsize="end"
        android:singleLine="true"
        android:layout_marginLeft="@dimen/margin_10dp"
        app:layout_constraintBottom_toBottomOf="@+id/more_item_head"
        app:layout_constraintLeft_toRightOf="@+id/more_item_head"
        android:layout_marginStart="@dimen/margin_10dp" />

    <TextView
        android:id="@+id/more_item_favorable"
        android:text="好评率：98%"
        android:textSize="11sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/margin_15dp"
        app:layout_constraintTop_toTopOf="@+id/more_item_name"
        app:layout_constraintBottom_toBottomOf="@+id/more_item_name"
        app:layout_constraintRight_toLeftOf="@+id/more_item_address"
        android:layout_marginEnd="@dimen/margin_15dp" />

    <TextView
        android:id="@+id/more_item_address"
        android:text="北京市朝阳区"
        android:textSize="11sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/margin_10dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/more_item_name"
        app:layout_constraintBottom_toBottomOf="@+id/more_item_name"
        android:layout_marginEnd="@dimen/margin_10dp" />

    <TextView
        android:id="@+id/more_item_set"
        android:text="设为专员"
        android:textColor="@color/white"
        android:padding="2dp"
        android:textSize="12sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_more_server_item"
        android:layout_marginRight="@dimen/margin_10dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/more_item_text"
        android:layout_marginEnd="@dimen/margin_10dp" />

    <ImageView
        android:id="@+id/more_server_card"
        android:layout_width="0dp"
        android:layout_height="130dp"
        android:layout_marginLeft="@dimen/margin_10dp"
        android:layout_marginRight="@dimen/margin_10dp"
        android:layout_marginTop="@dimen/margin_20dp"
        android:background="@drawable/shape_more_server_card"
        app:layout_constraintTop_toBottomOf="@+id/more_item_head"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="@dimen/margin_10dp"
        android:layout_marginEnd="@dimen/margin_10dp" />

    <TextView
        android:id="@+id/server_text"
        android:text="专员的一些吸引人的地方人的地方"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:singleLine="true"
        android:ellipsize="end"
        android:textColor="@color/black"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintVertical_bias="0.28"
        app:layout_constraintTop_toTopOf="@+id/more_server_card"
        app:layout_constraintBottom_toBottomOf="@+id/more_server_card"
        app:layout_constraintLeft_toLeftOf="@+id/more_server_card"
        app:layout_constraintRight_toRightOf="@+id/more_server_card"
        />

    <ImageView
        android:id="@+id/server_image"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@mipmap/brand_default"
        app:layout_constraintVertical_bias="0.35"
        android:layout_marginLeft="@dimen/margin_10dp"
        app:layout_constraintTop_toTopOf="@+id/more_server_card"
        app:layout_constraintBottom_toBottomOf="@+id/more_server_card"
        app:layout_constraintLeft_toRightOf="@+id/server_text"
        />

    <ImageView
        android:id="@+id/more_server_card2"
        android:layout_width="0dp"
        android:layout_height="130dp"
        android:layout_marginLeft="@dimen/margin_10dp"
        android:layout_marginRight="@dimen/margin_10dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/shape_more_server_card"
        app:layout_constraintTop_toBottomOf="@+id/server_text"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="@dimen/margin_10dp"
        android:layout_marginEnd="@dimen/margin_10dp" />

    <TextView
        android:id="@+id/server_text2"
        android:text="专员的一些吸引人的地方人的地方"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:singleLine="true"
        android:ellipsize="end"
        android:textColor="@color/black"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintVertical_bias="0.28"
        app:layout_constraintTop_toTopOf="@+id/more_server_card2"
        app:layout_constraintBottom_toBottomOf="@+id/more_server_card2"
        app:layout_constraintLeft_toLeftOf="@+id/more_server_card2"
        app:layout_constraintRight_toRightOf="@+id/more_server_card2"
        />

    <ImageView
        android:id="@+id/server_image2"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@mipmap/brand_default"
        app:layout_constraintVertical_bias="0.35"
        android:layout_marginLeft="@dimen/margin_10dp"
        app:layout_constraintTop_toTopOf="@+id/more_server_card2"
        app:layout_constraintBottom_toBottomOf="@+id/more_server_card2"
        app:layout_constraintLeft_toRightOf="@+id/server_text2"
        />

    <ImageView
        android:id="@+id/more_server_card3"
        android:layout_width="0dp"
        android:layout_height="130dp"
        android:layout_marginLeft="@dimen/margin_10dp"
        android:layout_marginRight="@dimen/margin_10dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/shape_more_server_card"
        app:layout_constraintTop_toBottomOf="@+id/server_text2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="@dimen/margin_10dp"
        android:layout_marginEnd="@dimen/margin_10dp" />

    <TextView
        android:id="@+id/server_text3"
        android:text="专员的一些吸引人的地方人的地方一共两句话"
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textColor="@color/black"
        android:singleLine="true"
        android:ellipsize="end"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintVertical_bias="0.35"
        app:layout_constraintTop_toTopOf="@+id/more_server_card3"
        app:layout_constraintBottom_toBottomOf="@+id/more_server_card3"
        app:layout_constraintLeft_toLeftOf="@+id/more_server_card3"
        app:layout_constraintRight_toRightOf="@+id/more_server_card3"
        />

    <ImageView
        android:id="@+id/server_image3"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@mipmap/brand_default"
        app:layout_constraintVertical_bias="0.35"
        android:layout_marginLeft="@dimen/margin_10dp"
        app:layout_constraintTop_toTopOf="@+id/more_server_card3"
        app:layout_constraintBottom_toBottomOf="@+id/more_server_card3"
        app:layout_constraintLeft_toRightOf="@+id/server_text3"
         />

    <TextView
        android:id="@+id/server_more"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="查看更多"
        android:layout_marginTop="@dimen/margin_10dp"
        app:layout_constraintTop_toBottomOf="@+id/server_image3"
        app:layout_constraintLeft_toLeftOf="@+id/more_server_card3"
        app:layout_constraintRight_toRightOf="@+id/more_server_card3"
        />

</android.support.constraint.ConstraintLayout>