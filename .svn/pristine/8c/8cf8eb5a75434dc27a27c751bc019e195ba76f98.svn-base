<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_f5f5f5"
    android:orientation="vertical">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="214dp"
                    android:background="@mipmap/person_bg">

                    <LinearLayout
                        android:id="@+id/ll_QR"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/x24"
                        android:layout_marginTop="@dimen/x24"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:background="@mipmap/erweima2x_03" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/x4"
                            android:text="专员\n二维码名片"
                            android:textColor="@color/white"
                            android:textSize="@dimen/text_size_13" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_go_promotion_center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/margin_10dp"
                        android:layout_marginTop="20dp"
                        android:text="服务专员中心"
                        android:textColor="@color/white"
                        android:textSize="13sp"
                        android:visibility="gone" />

                    <com.yst.onecity.view.roundedimageview.RoundedImageView
                        android:id="@+id/iv_headView"
                        android:layout_width="@dimen/x70"
                        android:layout_height="@dimen/x70"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="@dimen/margin_20dp"
                        android:scaleType="fitXY"
                        android:src="@mipmap/head_2"
                        app:border_color="@color/color_dddddd"
                        app:border_width="1px"
                        app:corner_radius="10dip"
                        app:mutate_background="true"
                        app:oval="true" />

                    <TextView
                        android:id="@+id/tv_identification_status"
                        android:layout_width="80dp"
                        android:layout_height="@dimen/x32"
                        android:layout_gravity="center|right"
                        android:layout_marginBottom="@dimen/margin_20dp"
                        android:background="@mipmap/weirenzheng"
                        android:gravity="center"
                        android:padding="@dimen/padding_10px"
                        android:text="未认证"
                        android:textColor="@color/white"
                        android:textSize="14sp" />


                    <TextView
                        android:id="@+id/tv_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="110dp"
                        android:drawablePadding="5dp"
                        android:drawableRight="@mipmap/bianji"
                        android:text="昵称"
                        android:textColor="@color/white"
                        android:textSize="15sp" />


                    <TextView
                        android:id="@+id/tv_phone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="136dp"
                        android:text="17700000000"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_13" />
                </FrameLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
                    android:layout_marginLeft="@dimen/x24"
                    android:layout_marginRight="@dimen/x24"
                    android:layout_marginTop="170dp"
                    android:background="@drawable/shape_solid_white">

                    <TextView
                        android:id="@+id/tv_my_orders"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="15dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/wodedingdan"
                        android:gravity="center"
                        android:text="@string/my_order"
                        android:textColor="#222222"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="1px"
                        android:layout_height="@dimen/x48"
                        android:layout_gravity="center"
                        android:background="#cecece" />

                    <TextView
                        android:id="@+id/tv_my_account"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="15dp"
                        android:layout_weight="1"
                        android:drawableTop="@mipmap/wodezhanghu"
                        android:gravity="center"
                        android:text="@string/my_account"
                        android:textColor="#222222"
                        android:textSize="12sp" />
                </LinearLayout>
            </FrameLayout>

            <RelativeLayout
                android:id="@+id/rl_my_payment"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x52"
                android:layout_marginTop="@dimen/margin_20dp"
                android:background="@color/white">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="19dp"
                    android:layout_height="19dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x12"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:layout_marginTop="@dimen/margin_20dp"
                    android:src="@mipmap/fukuanma" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentTop="true"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:layout_toEndOf="@+id/imageView2"
                    android:layout_toRightOf="@+id/imageView2"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/my_payment"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_invitate_register"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x52"
                android:background="@color/white">

                <ImageView
                    android:id="@+id/iv_register"
                    android:layout_width="19dp"
                    android:layout_height="19dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/x12"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:layout_marginTop="@dimen/margin_20dp"
                    android:src="@mipmap/yao" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentTop="true"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:layout_toEndOf="@+id/iv_register"
                    android:layout_toRightOf="@+id/iv_register"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/tv_invitate_register"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_my_publish"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:background="@color/white"
                android:visibility="gone">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:drawableLeft="@mipmap/wodefabu"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/my_publish"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_my_attention"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:background="@color/white"
                android:visibility="gone">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:drawableLeft="@mipmap/wodeguanzhu"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/my_attention"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <RelativeLayout
                android:visibility="gone"
                android:id="@+id/rl_my_tickets"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x52"
                android:background="@color/white">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/x12"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:drawableLeft="@mipmap/my_tickets"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/my_tickets"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <RelativeLayout
                android:visibility="gone"
                android:id="@+id/rl_my_collection"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x52"
                android:background="@color/white">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/x12"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:drawableLeft="@mipmap/wodeshoucang"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/my_collection"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_my_bank_card"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x52"
                android:background="@color/white">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/x12"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:drawableLeft="@mipmap/wodeyinhangka"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/my_bank_card"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_publish"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x52"
                android:background="@color/white">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/x12"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:drawableLeft="@mipmap/fabu2x_03"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/my_share"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_attache"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x52"
                android:background="@color/white">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/x12"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:drawableLeft="@mipmap/zhuanyuan2x_03"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/my_attache"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/rl_account_safe"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x52"
                android:background="@color/white">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignLeft="@+id/view3"
                    android:layout_alignParentTop="true"
                    android:layout_alignStart="@+id/view3"
                    android:layout_marginLeft="@dimen/x2"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:drawableLeft="@mipmap/zhanghuanquan"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/account_safe"
                    android:textColor="#222222"
                    android:textSize="14sp" />


                <View
                    android:id="@+id/view3"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_manager_address"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x52"
                android:background="@color/white">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/x12"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:drawableLeft="@mipmap/guanlishouhuodizhi"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/manager_address"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/my_news"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x52"
                android:background="@color/white"
                android:visibility="gone">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/x12"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:drawableLeft="@mipmap/wodexiaoxi"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/my_news"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <!-- 消息数量小圆点 -->
                <TextView
                    android:id="@+id/messageTagTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_alignRight="@+id/messages"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="55dp"
                    android:background="@drawable/shape_circle_red"
                    android:gravity="center"
                    android:padding="5dp"
                    android:text="0"
                    android:textColor="@color/white"
                    android:textSize="@dimen/textSize_12dp"
                    android:visibility="gone" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_about_us"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x52"
                android:layout_marginTop="@dimen/margin_20dp"
                android:background="@color/white">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/x12"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:drawableLeft="@mipmap/guanyuwomen"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/about_us"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_version_update"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x52"
                android:background="@color/white">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="@dimen/x12"
                    android:layout_marginRight="@dimen/x10"
                    android:drawableLeft="@mipmap/vsupdata"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/version_update"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_contact_customer_service"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x52"
                android:background="@color/white">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/x12"
                    android:layout_marginRight="@dimen/margin_10dp"
                    android:drawableLeft="@mipmap/lianxikefu"
                    android:drawablePadding="10dp"
                    android:drawableRight="@mipmap/nav_icon_next"
                    android:gravity="center_vertical"
                    android:text="@string/contact_customer_service"
                    android:textColor="#222222"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/heigh_fenge"
                    android:layout_marginLeft="@dimen/margin_10dp"
                    android:background="@color/color_dddddd" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/tv_logout"
                android:layout_width="match_parent"
                android:layout_height="@dimen/layout_height_40dp"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/x20"
                android:layout_marginLeft="@dimen/x32"
                android:layout_marginRight="@dimen/x32"
                android:layout_marginTop="@dimen/x20"
                android:background="@drawable/shape_logout_bg"
                android:gravity="center"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/layout_height_40dp"
                    android:drawableLeft="@mipmap/guanbi"
                    android:drawablePadding="5dp"
                    android:gravity="center"
                    android:text="@string/logout"
                    android:textColor="@color/white"
                    android:textSize="@dimen/textSize_16dp" />
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</LinearLayout>