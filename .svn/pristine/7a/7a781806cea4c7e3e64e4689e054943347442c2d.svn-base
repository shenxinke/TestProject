<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.yst.tianyimember.view.roundedimageview.RoundedImageView
        android:id="@+id/iv_invitate_head"
        android:layout_width="@dimen/x50"
        android:layout_height="@dimen/x50"
        android:layout_margin="@dimen/margin_10dp"
        android:scaleType="fitXY"
        android:src="@mipmap/head_2"
        app:border_color="@color/gray"
        app:border_width="1px"
        app:corner_radius="10dip"
        app:mutate_background="true"
        app:oval="true" />

    <TextView
        android:id="@+id/tv_invitate_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/iv_invitate_head"
        android:text="fffff00"
        android:textColor="@color/black"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/tv_invitate_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="@dimen/margin_10dp"
        android:text="fffff00"
        android:textColor="@color/black"
        android:textSize="15sp" />
</RelativeLayout>
