<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/llayout_bind_card"
    android:layout_width="match_parent"
    android:visibility="gone"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:layout_width="@dimen/x128"
        android:layout_height="@dimen/x128"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/x30"
        android:src="@mipmap/withdrawa_img_none" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_marginLeft="@dimen/x15"
        android:layout_marginRight="@dimen/x15"
        android:layout_marginTop="@dimen/x30"
        android:gravity="center"
        android:text="暂无银行卡信息"
        android:textSize="@dimen/x16" />

    <TextView
        android:id="@+id/add_bank_tv"
        style="@style/headHunter_button_tv"
        android:layout_marginLeft="@dimen/x40"
        android:layout_marginRight="@dimen/x40"
        android:layout_marginTop="@dimen/x45"
        android:text="添加银行卡" />

</LinearLayout>